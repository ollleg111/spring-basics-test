<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
         http://www.springframework.org/schema/beans/spring-beans.xsd
         http://www.springframework.org/schema/context
         http://www.springframework.org/schema/context/spring-context-4.1.xsd">

    <context:component-scan base-package="com"/>

    <bean id="orderService" class="com.classwork.OrderService">
<!--        <constructor-arg name="" value=""/>-->
    </bean>

    <bean id="orderDAO" class="com.classwork.OrderDAO">
        <!--        <constructor-arg name="" value=""/>-->
    </bean>




    <bean id="route" class="com.hw1.Route">
                <constructor-arg name="id" value=""/>
                <constructor-arg name="steps" value=""/>
    </bean>

    <bean id="service" class="com.hw1.Services">
                <constructor-arg name="id" value=""/>
                <constructor-arg name="name" value=""/>
                <constructor-arg name="paramsToCall" value=""/>
    </bean>

    <bean id="step" class="com.hw1.Step">
                <constructor-arg name="id" value=""/>
                <constructor-arg name="servicesFrom" value=""/>
                <constructor-arg name="servicesTo" value=""/>
                <constructor-arg name="paramsServiceFrom" value=""/>
                <constructor-arg name="getParamsServiceTo" value=""/>
    </bean>

</beans>